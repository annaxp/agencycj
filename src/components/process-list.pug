- var processes = [['Анлитическая работа', 'Корпоративные сайты, лендинги интернет магазины, промо сайты текст рыба, текста', 'a'], ['Составление технического задания', 'Корпоративные сайты, лендинги интернет магазины, промо сайты текст рыба, текста', 'tz'], ['Разработка', 'Корпоративные сайты, лендинги интернет магазины, промо сайты текст рыба, текста', 'd'], ['Тестирование и доработка', 'Корпоративные сайты, лендинги интернет магазины, промо сайты текст рыба, текста', 't']]
.process-list-wrapper
	.process-list#process-list
		- var i = 0
		each process in processes
			- i++
			- var title = process[0]
			- var desc = process[1]
			- var area = process[2]
			div(class='process-item  grid-area-'+area)
				.process-item__icon
					include ./process-icon
					+process-icon(i)
				.process-item__content
					.process-item__title= title
					.process-item__desc= desc
					